{
  "cli": {
    "version": ">= 16.8.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "android": {
        "buildType": "app-bundle",
        "gradleCommand": ":app:bundleRelease",
        "env": {
          "EXPO_KOTLIN_VERSION": "1.8.0"
        }
      },
      "ios": {
        "simulator": false
      },
      "autoIncrement": true
    }
  },
  "submit": {
    "production": {}
  }
}
